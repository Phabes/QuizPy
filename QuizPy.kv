#:kivy 1.0.9


<CategoryButton>:
    size_hint: 1, None
    height: 200
    on_press: root.click()

<CreateCategory>:
    size_hint: 1, None
    height: 200
    on_press: root.click()

<QuizButton>:
    size_hint: 1, None
    height: 200
    on_press: root.click()
<AllContainersBox>:
    orientation:'horizontal'
    id:all_container_box
    padding:5
<NewContainerBox>:
    orientation:'vertical'
    size_hint_y:1
    canvas.before:
        Color:
            rgba:rgba('#AAAAAA')
        Line:
            rectangle:[*self.pos,*self.size,]
    TextInput:
        id: container_name
        hint_text: "Name"
        size_hint: 1, None
        height: 40
        font_size: 20
        focus: True
        multiline: False
    BoxLayout:
        orientation:'vertical'
        id:answers_box
    Button:
        size_hint:1,0.1
        id:add_answer_but
        text:"+"
        on_press:root.add_answer()

<ContainerAnswerType>:
    orientation:'horizontal'
    TextInput:
        size_hint:0.8,None
        id: container_answer
        hint_text: "Answer"
        size_hint: 1, None
        height: 40
        font_size: 20
        focus: True
        multiline: False
    Button:
        size_hint:0.2,None
        height: 40
        text:"-"
        id:remove_answer_but
        on_press:root.remove_answer()
<ContainerTypeCheckboxes>:
    orientation:'horizontal'
    size_hint:1,0.1
    canvas.before:
        Color:
            rgba:rgba('#AAAAAA')
        Line:
            rectangle:[*self.pos,*self.size,]
    BoxLayout:
        orientation:'horizontal'
        Label:
            text_size: self.size
            halign: 'right'
            valign: 'middle'
            text:"2"
        CheckBox:
            group:"container_num"
            on_active:root.checkbox_choose(self,self.active,2)
    BoxLayout:
        orientation:'horizontal'
        Label:
            text_size: self.size
            halign: 'right'
            valign: 'middle'
            text:"3"
        CheckBox:
            group:"container_num"
            on_active:root.checkbox_choose(self,self.active,3)
    BoxLayout:
        orientation:'horizontal'
        Label:
            text_size: self.size
            halign: 'right'
            valign: 'middle'
            text:"4"
        CheckBox:
            group:"container_num"
            on_active:root.checkbox_choose(self,self.active,4)
    
<CreateScreen>
    BoxLayout:
        orientation: "vertical"
        id:screen_box
        size: root.width, root.height
        BoxLayout:
            orientation: "horizontal"
            size_hint: 1,None
            height: 80
            AnchorLayout:
                anchor_x: "left"
                size_hint: 0.5,1
                Label:
                    size_hint: 0.5,1
                    text: "CREATING NEW QUIZ"
            AnchorLayout:
                anchor_x: "right"
                size_hint: 0.5,1
                BoxLayout:
                    orientation: "horizontal"
                    size_hint: 0.5,1
                    Button:
                        text: "CANCEL"
                        size_hint: 0.5, 0.8
                        pos_hint: {"center_y": 0.5}
                        on_press: root.cancel_creating()
        BoxLayout:
            orientation:'vertical'
            id:screen_content_box
            Label:
                id: message_to_user
                size_hint: 1, None
                height: 80
            ScrollView:
                id: scroll_view
                size_hint: 1, None
                height: root.height - 160
                scroll_wheel_distance: 80
                GridLayout:
                    id: optionsGrid
                    cols: 2
                    size_hint: 1, None
                    height: self.minimum_height
<StartScreen>
    BoxLayout:
        orientation: "vertical"
        size: root.width, root.height
        BoxLayout:
            orientation: "horizontal"
            size_hint: 1,None
            height: 80
            AnchorLayout:
                anchor_x: "left"
                size_hint: 0.5,1
                Label:
                    id: user_hello
                    size_hint: 0.5, 1
            AnchorLayout:
                anchor_x: "right"
                size_hint: 0.5,1
                BoxLayout:
                    orientation: "horizontal"
                    size_hint: 0.5,1
                    Button:
                        text: "CREATE QUIZ"
                        size_hint: 0.5, 0.8
                        pos_hint: {"center_y": 0.5}
                        on_press: root.manager.create_quiz()
                    Button:
                        text: "LOGOUT"
                        size_hint: 0.5, 0.8
                        pos_hint: {"center_y": 0.5}
                        on_press: root.manager.logout_user()
        ScrollView:
            size_hint: 1,None
            height: root.height - 80
            scroll_wheel_distance: 80
            GridLayout:
                id: categoriesGrid
                cols: 2
                size_hint: 1, None
                height: self.minimum_height

<CategoryScreen>
    BoxLayout:
        orientation: "vertical"
        size: root.width, root.height
        BoxLayout:
            orientation: "horizontal"
            size_hint: 1,None
            height: 80
            AnchorLayout:
                anchor_x: "left"
                size_hint: 0.5, 1
                BoxLayout:
                    size_hint: 1, 1
                    Button:
                        id: back_button
                        text: "BACK"
                        size_hint: 0.5, 0.8
                        pos_hint: {"center_y": 0.5}
                    BoxLayout:
                        size_hint: 0.5, 0.8
                        pos_hint: {"center_y": 0.5}
                        Label:
                            id: user_hello
                            size_hint: 1, 1
            AnchorLayout:
                anchor_x: "right"
                size_hint: 0.5, 1
                BoxLayout:
                    orientation: "horizontal"
                    size_hint: 0.5,1
                    Button:
                        text: "CREATE QUIZ"
                        size_hint: 0.5, 0.8
                        pos_hint: {"center_y": 0.5}
                        on_press: root.manager.create_quiz()
                    Button:
                        text: "LOGOUT"
                        size_hint: 0.5, 0.8
                        pos_hint: {"center_y": 0.5}
                        on_press: root.manager.logout_user()
        AnchorLayout:
            anchor_x:'center'
            anchor_y:'top'
            height: 80
            size_hint:1, 1
            GridLayout:
                cols: 2
                BoxLayout:
                    Label:
                        font_size: 20
                        center_y: root.height*90/100
                        text_size: self.size
                        size: root.width*0.95, 50
                        text: "YOUR QUIZES:"
                        background_color: 1, 0, 0, 1
                        halign: 'center'
                        valign: 'middle'
                    CheckBox:
                        id: my_filter
                        color: 1, 1, 0, 1
                AnchorLayout:
                    anchor_x:'center'
                    anchor_y:'center'
                    TextInput:
                        id: name_filter
                        size_hint: 0.8, None
                        height: 30
                        hint_text: "Quiz name filter"
        ScrollView:
            size_hint: 1,None
            height: root.height - 160
            scroll_wheel_distance: 80
            GridLayout:
                id: quiz_grid
                cols: 2
                size_hint: 1, None
                height: self.minimum_height

