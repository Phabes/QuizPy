#:kivy 1.0.9

<QuizPyGame>:
    Label:
        font_size:70
        center_x:root.width/2
        top: root.top -50
        text: "0"
    
    Label:
        font_size:70
        center_x: root.width*1/4
        center_y:root.height*5/10
        # top:root.top-50
        text: "A"
    Label:
        font_size:70
        center_x: root.width*3/4
        center_y:root.height*5/10
        # top:root.top-50
        text: "B"
    Label:
        font_size:70
        center_x: root.width*1/4
        center_y:root.height*3/10
        # top:root.top-50
        text: "C"
    Label:
        font_size:70
        center_x: root.width*3/4
        center_y:root.height*3/10
        # top:root.top-50
        text: "D"
