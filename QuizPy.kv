<DraggableLabel>:
    drag_clas:'answer'
    drag_timeout:0
    font_size:'40sp'
    text: self.text
    canvas.before:
        Color:
            rgba:rgba("#7FFFD4") if self.is_being_dragged else rgba("#40E0D0")
        Rectangle:
            size: self.size
            pos: self.pos
<Container>:
    orientation:'vertical'
    Label:
        font_size: 20
        center_y: root.height*90/100
        text_size: self.size
        size: root.width*0.95, 50
        text: root.text
        background_color: 1, 0, 0, 1
        halign: 'center'
        valign: 'middle'
    CategoryBox:
        drag_cls:'answer'
        drag_classes:['answer',]
        answer_option:root.answer_option
        parentObject:root.parentObject
        canvas.before:
            Color:
                rgba:rgba('#AAAAAA')
            Line:
                rectangle:[*self.pos,*self.size,]
<ChooseContainerScreen>:
    BoxLayout:
        orientation: "vertical"
        size: root.width, root.height
        spacing:5
        padding:5
        BoxLayout:
            orientation: "horizontal"
            size_hint: 1,None
            height: 80
            AnchorLayout:
                anchor_x: "left"
                size_hint: 0.5, 1
                BoxLayout:
                    size_hint: 1, 1
                    Button:
                        id: back_button
                        text: "QUIT"
                        size_hint: 0.5, 0.8
                        pos_hint: {"center_y": 0.5}
                    BoxLayout:
                        size_hint: 0.5, 0.8
                        pos_hint: {"center_y": 0.5}
                        Label:
                            id: user_hello
                            size_hint: 1, 1
            AnchorLayout:
                anchor_x: "right"
                size_hint: 0.5, 1
                BoxLayout:
                    orientation: "horizontal"
                    size_hint: 0.5,1
                    Button:
                        text: "CREATE QUIZ"
                        size_hint: 0.5, 0.8
                        pos_hint: {"center_y": 0.5}
                        on_press: root.manager.create_quiz()
                    Button:
                        text: "LOGOUT"
                        size_hint: 0.5, 0.8
                        pos_hint: {"center_y": 0.5}
                        on_press: root.manager.logout_user()
        AnchorLayout:
            anchor_x:'center'
            anchor_y:'top'
            size_hint:1, 1
            GridLayout:
                rows: 2
                size_hint: 1, 1
                GridLayout:
                    cols: 2
                    size_hint: 1, 1
                    Label:
                        id: user_points
                        font_size: 20
                        center_y: root.height*90/100
                        text_size: self.size
                        size: root.width*0.95, 50
                        text: ""
                        background_color: 1, 0, 0, 1
                        halign: 'center'
                        valign: 'middle'
                    Label:
                        id: remaining_time
                        font_size: 20
                        center_y: root.height*90/100
                        text_size: self.size
                        size: root.width*0.95, 50
                        text: ""
                        background_color: 1, 0, 0, 1
                        halign: 'center'
                        valign: 'middle'
                Label:
                    id: main_question
                    font_size:root.width/25
                    center_y:root.height*90/100
                    text_size:self.size
                    size:root.width*0.95,150
                    text: "[Error]"
                    background_color: 1, 0, 0, 1
                    halign: 'center'
                    valign: 'middle'
        AnchorLayout:
            anchor_x:'center'
            anchor_y:'top'
            size_hint:1,0.3
            Label:
                id: after_answer_label
                visible: False
                font_size:root.width/20
                center_x:root.width/2
                center_y:root.height*80/100
                text: "[Error]"
                size_hint_x: 1 if self.visible else 0
                opacity: 1 if self.visible else 0
                disabled: not self.visible
        BoxLayout:
            orientation:'horizontal'
            padding:10
            spacing:10
            size:root.width,root.height*0.2
            id:answer_destination_fields
        GridLayout:
            id:answer_grid
            cols:3
            padding:20
            spacing:20

<AnswerBox>:
    drag_cls:'answer'
    drag_classes:['answer',]
    parentObject:root.parentObject
    canvas.before:
        Color:
            rgba:rgba('#AAAAAA')
        Line:
            rectangle:[*self.pos,*self.size,]  
<DraggableLabel>:
    drag_cls:'answer'
    drag_timeout:0
    font_size:'40sp'
    background_color:"#00ff00" #if self.isCorrect==0 else ( "#ff0000" if self.isCorrect>=0 else "#AAAAAA" )
