<LoginScreen>
    BoxLayout:
        AnchorLayout:
            anchor_x: "center"
            anchor_y: "center"
            BoxLayout:
                size_hint: 0.8, 0.8
                size: root.width*0.8, root.height*0.8
                GridLayout:
                    rows: 5
                    cols: 1
                    size_hint: 1, 1
                    TextInput:
                        id: login_input
                        hint_text: "login"
                        size_hint: 1, None
                        height: 40
                        font_size: 20
                        focus: True
                        multiline: False
                    TextInput
                        id: password_input
                        hint_text: "password"
                        password: True
                        size_hint: 1, None
                        height: 40
                        font_size: 20
                        multiline: False
                    Button:
                        id: login_button
                        text: "LOGIN"
                        size_hint: 0.5, None
                        height: 50
                        pos_hint: {"center_x": 0.5}
                        on_press: root.login_button_click()
                    Label:
                        text: "Don't have account?"
                    Button:
                        text: "Create Account"
                        size_hint: 0.5, None
                        height: 50
                        pos_hint: {"center_x": 0.5}
                        on_press: root.manager.current = "register"

<RegisterScreen>
    BoxLayout:
        AnchorLayout:
            anchor_x: "center"
            anchor_y: "center"
            BoxLayout:
                size_hint: 0.8, 0.8
                size: root.width*0.8, root.height*0.8
                GridLayout:
                    rows: 6
                    cols: 1
                    size_hint: 1, 1
                    TextInput:
                        id: login_input
                        hint_text: "login"
                        size_hint: 1, None
                        height: 40
                        font_size: 20
                        focus: True
                        multiline: False
                    TextInput:
                        id: password_input
                        hint_text: "password"
                        password: True
                        size_hint: 1, None
                        height: 40
                        font_size: 20
                        multiline: False
                    TextInput:
                        id: confirm_password_input
                        hint_text: "password"
                        password: True
                        size_hint: 1, None
                        height: 40
                        font_size: 20
                        multiline: False
                    Button:
                        id: login_button
                        text: "REGISTER"
                        size_hint: 0.5, None
                        height: 50
                        pos_hint: {"center_x": 0.5}
                        on_press: root.register_button_click()
                    Label:
                        text: "Already have account?"
                    Button:
                        text: "Go to Login"
                        size_hint: 0.5, None
                        height: 50
                        pos_hint: {"center_x": 0.5}
                        on_press: root.manager.current = "login"